CREATE EXTERNAL TABLE deutsche_<your-name> (
ISIN string,
Mnemonic string,
SecurityType string,
Currency string,
SecurityID int,
`Date` string,
Time string,
StartPrice double,
MaxPrice double,
MinPrice double,
EndPrice double,
TradedVolume double,
NumberOfTrades int)
STORED AS PARQUET
LOCATION "<your S3 data diretory path>"
